          "_name": "slider1",
          "_sourceTheme": "successm4",
          "_customHTML": "<section class=\"slider1 clients\" group=\"Sliders & Galleries\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\" plugins=\"BootstrapCarouselSwipe, mbr-clients-slider\" data-interval=\"false\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"6\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"9\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Show Tag\" name=\"showTag\" checked>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Second Color Title\" name=\"secondColorTitle\" checked condition=\"showTitle\">\n        <input type=\"color\" title=\"Second Color Title\" value=\"#f23801\" name=\"secondColor\" selected condition=\"secondColorTitle && showTitle\">\n        <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\">\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showText\">\n        <header>Slider</header>\n        <input type=\"range\" inline title=\"Items\" name=\"slidesCount\" min=\"1\" max=\"20\" step=\"1\" value=\"5\">\n        <select title=\"Visible Slides\" name=\"visibleSlides\" condition=\"false\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\" selected>3</option>\n        </select>\n        <input type=\"checkbox\" title=\"Autoplay\" name=\"showAutoplay\" checked>\n        <input type=\"range\" inline title=\"Autoplay Interval (ms)\" name=\"interval\" min=\"1000\" max=\"8000\" step=\"1000\" value=\"4000\" condition=\"showAutoplay\">\n        <input type=\"checkbox\" title=\"Show Arrows\" name=\"showArrows\">\n        <input type=\"color\" title=\"Arrows Color\" name=\"arrowsColor\" value=\"#f23801\" condition=\"showArrows\">\n          <input type=\"range\" title=\"Arrow Size\" name=\"arrowsSize\" value=\"7\" min=\"1\" max=\"10\" step=\"1\" condition=\"showArrows\">\n        <input type=\"checkbox\" title=\"Show Dots\" name=\"dots\" checked>        \n        <input type=\"color\" title=\"Dots Color\" name=\"dotsColor\" value=\"#000000\" condition=\"dots\">\n        <input type=\"color\" title=\"Dots Color Active\" name=\"dotsColorActive\" value=\"#000000\" condition=\"dots\">\n        <input type=\"range\" title=\"Dots Opacity\" name=\"dotsOpacity\" min=\"0\" max=\"10\" step=\"1\" value=\"10\" condition=\"dots\">\n        <header>Card</header>\n        <input type=\"checkbox\" title=\"Card Image\" name=\"cardImage\" checked>\n        <input type=\"checkbox\" title=\"Card Subtitle\" name=\"cardSubtitle\" checked>\n        <input type=\"checkbox\" title=\"Card Title\" name=\"cardTitle\" checked>\n        <input type=\"checkbox\" title=\"Card Text\" name=\"cardText\" checked>\n        <input type=\"checkbox\" title=\"Card Buttons\" name=\"cardButtons\">\n        <input type=\"checkbox\" title=\"Card Hover\" name=\"cardHover\" checked>\n        <select title=\"Hover Style\" name=\"hoverStyle\" condition=\"cardHover\">\n            <option value=\"1\" selected>1</option>\n            <option value=\"2\">2</option>\n        </select>\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@THEME_PATH@/components/_images/background.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n            <input type=\"video\" title=\"Video\" value=\"https://www.youtube.com/watch?v=36YgDDJ7XSc\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.9\" condition=\"overlay && bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div class=\"container mbr-content\" mbr-if=\"showTitle || showText || showSubtitle || showTag\">\n        <div class=\"row\">\n            <div class=\"col-12\">\n                <p class=\"mbr-section-tag mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showTag\" data-app-selector=\".mbr-section-tag\">PROJECTS</p>\n                <h3 class=\"mbr-section-title figure mbr-fonts-style w-100\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\"><b>Featured&nbsp;</b><b>Projects Showcase</b></h3>\n                <p class=\"mbr-text mbr-section-subtitle mbr-fonts-style mt-0\" mbr-theme-style=\"display-5\" data-app-selector=\".mbr-section-subtitle\" mbr-if=\"showSubtitle\"><i>Lorem ipsum dolor sit amet,\n                        consectetur adipisicing elit.</i></p>\n                <p class=\"mbr-text mbr-fonts-style mt-0\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-text\" mbr-if=\"showText\">Lorem ipsum dolor <b>sit amet, consectetur </b>adipisicing elit. Quis debitis,\n                    illo ex cupiditate error laboriosam earum repellendus amet facere fugiat, similique, et aut\n                    obcaecati quisquam beatae. Odit, explicabo, excepturi. Quae.</p>\n            </div>\n        </div>\n    </div>\n\n    <div class=\"container\">\n        <div class=\"carousel slide\" role=\"listbox\" data-pause=\"true\" data-keyboard=\"false\" data-ride=\"{{showAutoplay ? 'carousel' : 'false'}}\" data-interval=\"{{showAutoplay ? interval : 'false'}}\">\n            <ol class=\"carousel-indicators\" mbr-if=\"dots\">\n                <li data-slide-to=\"0\" class=\"active\">\n                <svg class=\"qodef-svg-circle\"><circle cx=\"50%\" cy=\"50%\" r=\"45%\"></circle></svg></li>\n                <li data-slide-to=\"1\" mbr-if=\"slidesCount > 1\"><svg class=\"qodef-svg-circle\"><circle cx=\"50%\" cy=\"50%\" r=\"45%\"></circle></svg></li>\n                <li data-slide-to=\"2\" mbr-if=\"slidesCount > 2\"><svg class=\"qodef-svg-circle\"><circle cx=\"50%\" cy=\"50%\" r=\"45%\"></circle></svg></li>\n                <li data-slide-to=\"3\" mbr-if=\"slidesCount > 3\"><svg class=\"qodef-svg-circle\"><circle cx=\"50%\" cy=\"50%\" r=\"45%\"></circle></svg></li>\n                <li data-slide-to=\"4\" mbr-if=\"slidesCount > 4\"><svg class=\"qodef-svg-circle\"><circle cx=\"50%\" cy=\"50%\" r=\"45%\"></circle></svg></li>\n                <li data-slide-to=\"5\" mbr-if=\"slidesCount > 5\"><svg class=\"qodef-svg-circle\"><circle cx=\"50%\" cy=\"50%\" r=\"45%\"></circle></svg></li>\n                <li data-slide-to=\"6\" mbr-if=\"slidesCount > 6\"><svg class=\"qodef-svg-circle\"><circle cx=\"50%\" cy=\"50%\" r=\"45%\"></circle></svg></li>\n                <li data-slide-to=\"7\" mbr-if=\"slidesCount > 7\"><svg class=\"qodef-svg-circle\"><circle cx=\"50%\" cy=\"50%\" r=\"45%\"></circle></svg></li>\n                <li data-slide-to=\"8\" mbr-if=\"slidesCount > 8\"><svg class=\"qodef-svg-circle\"><circle cx=\"50%\" cy=\"50%\" r=\"45%\"></circle></svg></li>\n                <li data-slide-to=\"9\" mbr-if=\"slidesCount > 9\"><svg class=\"qodef-svg-circle\"><circle cx=\"50%\" cy=\"50%\" r=\"45%\"></circle></svg></li>\n                <li data-slide-to=\"10\" mbr-if=\"slidesCount > 10\"><svg class=\"qodef-svg-circle\"><circle cx=\"50%\" cy=\"50%\" r=\"45%\"></circle></svg></li>\n                <li data-slide-to=\"11\" mbr-if=\"slidesCount > 11\"><svg class=\"qodef-svg-circle\"><circle cx=\"50%\" cy=\"50%\" r=\"45%\"></circle></svg></li>\n                <li data-slide-to=\"12\" mbr-if=\"slidesCount > 12\"><svg class=\"qodef-svg-circle\"><circle cx=\"50%\" cy=\"50%\" r=\"45%\"></circle></svg></li>\n                <li data-slide-to=\"13\" mbr-if=\"slidesCount > 13\"><svg class=\"qodef-svg-circle\"><circle cx=\"50%\" cy=\"50%\" r=\"45%\"></circle></svg></li>\n                <li data-slide-to=\"14\" mbr-if=\"slidesCount > 14\"><svg class=\"qodef-svg-circle\"><circle cx=\"50%\" cy=\"50%\" r=\"45%\"></circle></svg></li>\n                <li data-slide-to=\"15\" mbr-if=\"slidesCount > 15\"><svg class=\"qodef-svg-circle\"><circle cx=\"50%\" cy=\"50%\" r=\"45%\"></circle></svg></li>\n                <li data-slide-to=\"16\" mbr-if=\"slidesCount > 16\"><svg class=\"qodef-svg-circle\"><circle cx=\"50%\" cy=\"50%\" r=\"45%\"></circle></svg></li>\n                <li data-slide-to=\"17\" mbr-if=\"slidesCount > 17\"><svg class=\"qodef-svg-circle\"><circle cx=\"50%\" cy=\"50%\" r=\"45%\"></circle></svg></li>\n                <li data-slide-to=\"18\" mbr-if=\"slidesCount > 18\"><svg class=\"qodef-svg-circle\"><circle cx=\"50%\" cy=\"50%\" r=\"45%\"></circle></svg></li>\n                <li data-slide-to=\"19\" mbr-if=\"slidesCount > 19\"><svg class=\"qodef-svg-circle\"><circle cx=\"50%\" cy=\"50%\" r=\"45%\"></circle></svg></li>\n            </ol>\n            <div class=\"slider-clients\">\n                <div class=\"carousel-inner\" mbr-list mbr-list-grow=\"slidesCount\" data-visible=\"{{visibleSlides}}\">\n                    <div class=\"carousel-item\">\n                        <div class=\"media-container-row\">\n                            <div class=\"slider__item col-md-12\">\n                                <div class=\"wrap-img\">\n                                    <div class=\"slider__image\" mbr-if=\"cardImage\">\n                                        <img src=\"@PROJECT_PATH@/assets/images/feature53.jpg\" alt title>\n                                    </div>\n                                    <div class=\"slider__content\" mbr-if=\"cardSubtitle || cardTitle || cardText || cardButtons\">\n                                        <div class=\"slider__content-wrap\">\n                                            <p class=\"slider__subtitle mbr-fonts-style mb-0\" mbr-if=\"cardSubtitle\" mbr-theme-style=\"display-7\" data-app-selector=\".slider__content .slider__subtitle\"><a href=\"index.html\" class=\"text-info\">Business</a></p>\n                                            <h5 class=\"slider__title mbr-fonts-style\" mbr-if=\"cardTitle\" mbr-theme-style=\"display-5\" data-app-selector=\".slider__content .slider__title\"><b>Project #1</b></h5>\n                                            <p class=\"slider__text mbr-fonts-style\" mbr-if=\"cardText\" mbr-theme-style=\"display-7\" data-app-selector=\".slider__content .slider__text, .link\">Lorem ipsum dolor sit amet, conse ctetuer adipiscing elit, sed diam nonum nibhie</p>\n                                        </div>\n                                        <p class=\"link mbr-fonts-style mt-3\" mbr-if=\"cardButtons\" mbr-theme-style=\"display-4\" data-app-selector=\".link\"><a href=\"#\">LEARN MORE<span mbr-icon class=\"fa-long-arrow-right fa mbr-iconfont\"></span></a>\n                                        </p>\n                                    </div>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                    \n                    \n                    \n                    \n                <div class=\"carousel-item\">\n                        <div class=\"media-container-row\">\n                            <div class=\"slider__item col-md-12\">\n                                <div class=\"wrap-img\">\n                                    <div class=\"slider__image\" mbr-if=\"cardImage\">\n                                        <img src=\"@PROJ