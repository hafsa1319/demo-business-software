          "_customHTML": "<section class=\"form2\" group=\"Forms\" plugins=\"formstyler, datepicker\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <header>Text</header>\n        <input type=\"checkbox\" title=\"Reverse\" name=\"reverse\">\n        <input type=\"range\" inline title=\"Text Width\" name=\"textWidth\" min=\"4\" max=\"10\" step=\"1\" value=\"6\">\n        <input type=\"checkbox\" title=\"Tag\" name=\"showTagText\" checked>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitleText\" checked>\n        <input type=\"checkbox\" title=\"Second Color Title\" name=\"secondColorTitle\" condition=\"showTitleText\" checked>\n        <input type=\"color\" title=\"Second Color Title\" value=\"#f23801\" name=\"secondColor\" selected condition=\"secondColorTitle && showTitleText\">\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitleText\" checked>\n        <input type=\"checkbox\" title=\"Text\" name=\"showTextText\" checked>\n        <input type=\"range\" title=\"Item Count\" name=\"items\" min=\"0\" max=\"3\" step=\"1\" value=\"0\">\n        <input type=\"range\" title=\"Icons Count\" name=\"iconCount\" min=\"0\" max=\"5\" step=\"1\" value=\"3\" condition=\"items > 1\">\n        <input type=\"color\" title=\"Border Icon Color\" name=\"borderIconColor\" value=\"#000000\" condition=\"iconCount > 0 &amp;&amp; items > 1\">\n        <input type=\"color\" title=\"Icon Color\" name=\"iconColor\" value=\"#000\" condition=\"iconCount > 0 &amp;&amp; items > 1\">\n        <input type=\"color\" title=\"Icon Color Hover\" name=\"iconColorHover\" value=\"#f23801\" condition=\"iconCount > 0 &amp;&amp; items > 1\">\n        <input type=\"color\" title=\"Color Border Form\" value=\"#000000\" name=\"colorBorder\" selected>\n        <header>Form</header>\n        <input type=\"range\" inline title=\"Form Width\" name=\"formWidth\" min=\"4\" max=\"10\" step=\"1\" value=\"6\">\n        <input type=\"checkbox\" title=\"Titile Form\" name=\"showTitileForm\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Image\" value=\"@THEME_PATH@/components/_images/background1.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#fafafa\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.4\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"hidden\" name=\"usedRecaptcha\" value=\"false\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay && bg.type=='image')\"></div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row mt-4\">\n              <div class=\"col-md-12 col-lg-{{textWidth}}\" mbr-class=\"{'order-2' : reverse, 'pl-lg-5' : reverse, 'pr-lg-5' : !reverse}\">\n                <div class=\"text-wrapper\">\n                    <p class=\"mbr-fonts-style mbr-section-tag\" mbr-theme-style=\"display-7\" mbr-if=\"showTagText\" data-app-selector=\".mbr-section-tag\">CONTACTS</p>\n                    <h2 class=\"mbr-section-title mbr-fonts-style w-100\" mbr-theme-style=\"display-2\" mbr-if=\"showTitleText\" data-app-selector=\".mbr-section-title\"><b>Let's Develop</b><br><b>Your Next Great App!</b></h2>\n                   <p class=\"mbr-fonts-style mbr-section-subtitle w-100\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitleText\" data-app-selector=\".mbr-section-subtitle, .mbr-section-btn\"><i>Lorem ipsum dolor sit amet, conse ctetuer adipiscing elit, sed diam nonum nibhie</i><br></p>\n                    <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showTextText\" data-app-selector=\".mbr-text, .mbr-section-btn\">Lorem ipsum dolor sit amet, conse ctetuer adipiscing elit, sed diam nonum nibhie<br></p>\n                    <div class=\"text-wrapper w-100 row\" mbr-if=\"items > 0\">\n                        <div class=\"mbr-item flex-column col-md-6\" mbr-if=\"items > 0\">\n                            <p class=\"mbr-text mbr-fonts-style mbr-section-tag-item mb-0\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-section-tag-item, .mbr-social-likns\">CONTACT ADDRESS</p>\n                            <p class=\"mbr-section-link mbr-fonts-style mb-0 mbr-semibold\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-section-link\">1234 Street Name \n<br>City, AA 99999<br></p>\n                        </div>\n                        <div class=\"mbr-item flex-column col-md-6\" mbr-if=\"items > 1\">\n                           <p class=\"mbr-text mbr-fonts-style mbr-section-tag-item mb-0\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-section-tag-item, .mbr-social-likns\">SOCIAL MEDIA</p>\n                          <div class=\"mbr-social-likns align-left mb-0\" data-counters=\"false\" mbr-if=\"iconCount > 0\"> \n                            <a class=\"iconfont-wrapper ml-0\" href=\"https://mobiri.se\" target=\"_blank\" mbr-if=\"iconCount > 0\">\n                                <span mbr-icon class=\"p-2 ml-0 mbr-iconfont socicon-facebook socicon\"></span>\n                            </a>\n                            <a class=\"iconfont-wrapper\" href=\"https://mobiri.se\" target=\"_blank\" mbr-if=\"iconCount > 1\">\n                                <span mbr-icon class=\"p-2 mbr-iconfont socicon-linkedin socicon\"></span>\n                            </a>\n                            <a class=\"iconfont-wrapper\" href=\"https://mobiri.se\" target=\"_blank\" mbr-if=\"iconCount > 2\">\n                                <span mbr-icon class=\"p-2 mbr-iconfont socicon-twitter socicon\"></span>\n                            </a>\n                            <a class=\"iconfont-wrapper\" href=\"https://mobiri.se\" target=\"_blank\" mbr-if=\"iconCount > 3\">\n                                <span mbr-icon class=\"p-2 mbr-iconfont socicon-youtube socicon\"></span>\n                            </a>\n                            <a class=\"iconfont-wrapper\" href=\"https://mobiri.se\" target=\"_blank\" mbr-if=\"iconCount > 4\">\n                                <span mbr-icon class=\"p-2 mbr-iconfont socicon-pinterest socicon\"></span>\n                            </a>\n                        </div>\n                    </div>\n                    <div class=\"mbr-item flex-column col-md-6\" mbr-if=\"items > 2\">\n                        <p class=\"mbr-text mbr-fonts-style mbr-section-tag-item mb-0\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-section-tag-item, .mbr-social-likns\">GENERAL INQUIRIES</p>\n                        <p class=\"mbr-section-link mbr-fonts-style mb-0 mbr-semibold\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-section-link\"><a href=\"index.html\" class=\"text-black\">example@example.com</a></p>\n                    </div>\n                </div>\n                </div>\n            </div>\n            <div mbr-form class=\"col-12 col-lg-{{formWidth}} my-5 my-lg-0 mbr-form\">\n                <p class=\"mbr-title-form mbr-fonts-style align-left\" mbr-theme-style=\"display-5\" mbr-if=\"showTitileForm\" data-app-selector=\".mbr-title-form\">Leave a request, our manager will contact you within 5 minutes<br></p>\n                <div mbr-form>\n                <form action name=\"Form Name\" method=\"POST\" data-rc